{
	"name": "tarikicode-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@material/theme": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui/button": "^7.0.0-beta.16",
		"@smui/card": "^7.0.0-beta.16",
		"@smui/checkbox": "^7.0.0-beta.16",
		"@smui/fab": "^7.0.0-beta.16",
		"@smui/form-field": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.16",
		"@smui/layout-grid": "^7.0.0-beta.16",
		"@smui/menu": "^7.0.0-beta.16",
		"@smui/paper": "^7.0.0-beta.16",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/app-utils": "^1.0.0-next.0",
		"@sveltejs/kit": "^2.5.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"i": "^0.3.7",
		"npm": "^10.5.0",
		"smui-theme": "^7.0.0-beta.16",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
