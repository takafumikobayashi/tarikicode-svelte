{
	"name": "tarikicode-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint --fix .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"lint:md": "markdownlint-cli2 **/*.md !node_modules !static",
		"fix:md": "markdownlint-cli2 --fix **/*.md !node_modules !static"
	},
	"devDependencies": {
		"@iconify-json/cib": "^1.2.0",
		"@iconify-json/ic": "^1.2.0",
		"@iconify-json/logos": "^1.2.0",
		"@material/theme": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui/button": "^7.0.0-beta.16",
		"@smui/card": "^7.0.0-beta.16",
		"@smui/checkbox": "^7.0.0-beta.16",
		"@smui/data-table": "^7.0.0-beta.18",
		"@smui/dialog": "^7.0.0",
		"@smui/fab": "^7.0.0-beta.16",
		"@smui/form-field": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.16",
		"@smui/layout-grid": "^7.0.0-beta.16",
		"@smui/menu": "^7.0.0-beta.16",
		"@smui/paper": "^7.0.0-beta.16",
		"@smui/ripple": "^7.0.0",
		"@smui/tab": "^7.0.0-beta.18",
		"@smui/tab-bar": "^7.0.0-beta.18",
		"@smui/textfield": "^7.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/app-utils": "^1.0.0-next.0",
		"@sveltejs/kit": "^2.5.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/marked": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"@vitest/ui": "^3.2.4",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"happy-dom": "^19.0.2",
		"highlight.js": "^11.9.0",
		"i": "^0.3.7",
		"jsdom": "^27.0.0",
		"markdownlint-cli2": "^0.18.1",
		"marked": "^12.0.1",
		"mermaid": "^11.2.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"smui-theme": "^7.0.0-beta.16",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"swiper": "^11.0.7",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"openai": "^6.1.0"
	}
}
