{
	"name": "tarikicode-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"lint": "prettier --check . && eslint . && npm run lint:md",
		"lint:fix": "prettier --write . && eslint --fix . && npm run fix:md",
		"format": "prettier --write .",
		"lint:code": "prettier --check . && eslint .",
		"lint:code:fix": "prettier --write . && eslint --fix .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"lint:md": "markdownlint-cli2 **/*.md !node_modules !static",
		"fix:md": "markdownlint-cli2 --fix **/*.md !node_modules !static"
	},
	"devDependencies": {
		"@iconify-json/cib": "^1.2.0",
		"@iconify-json/ic": "^1.2.0",
		"@iconify-json/logos": "^1.2.0",
		"@iconify-json/vscode-icons": "^1.2.42",
		"@material/theme": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui/button": "^8.0.3",
		"@smui/card": "^8.0.3",
		"@smui/checkbox": "^8.0.3",
		"@smui/data-table": "^8.0.3",
		"@smui/dialog": "^8.0.3",
		"@smui/fab": "^8.0.3",
		"@smui/form-field": "^8.0.3",
		"@smui/icon-button": "^8.0.3",
		"@smui/layout-grid": "^8.0.3",
		"@smui/menu": "^8.0.3",
		"@smui/paper": "^8.0.3",
		"@smui/ripple": "^8.0.3",
		"@smui/tab": "^8.0.3",
		"@smui/tab-bar": "^8.0.3",
		"@smui/textfield": "^8.0.3",
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/adapter-vercel": "^6.3.2",
		"@sveltejs/app-utils": "^1.0.0-next.0",
		"@sveltejs/kit": "^2.5.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/dompurify": "^3.2.0",
		"@types/leaflet": "^1.9.21",
		"@types/marked": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"@vitest/ui": "^4.0.18",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^17.3.0",
		"highlight.js": "^11.9.0",
		"i": "^0.3.7",
		"markdownlint-cli2": "^0.21.0",
		"marked": "^17.0.3",
		"mermaid": "^11.2.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"smui-theme": "^8.0.3",
		"svelte": "^4.2.7",
		"svelte-check": "^4.4.4",
		"svelte-preprocess": "^6.0.3",
		"swiper": "^12.1.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^4.0.18"
	},
	"overrides": {
		"cookie": "^0.7.0"
	},
	"type": "module",
	"dependencies": {
		"@smui/chips": "^8.0.3",
		"@smui/circular-progress": "^8.0.3",
		"@smui/select": "^8.0.3",
		"@smui/snackbar": "^8.0.3",
		"@smui/tooltip": "^8.0.3",
		"chart.js": "^4.5.1",
		"chartjs-plugin-datalabels": "^2.2.0",
		"dompurify": "^3.3.0",
		"gray-matter": "^4.0.3",
		"happy-dom": "^20.7.0",
		"iconv-lite": "^0.7.0",
		"leaflet": "^1.9.4",
		"openai": "^6.1.0",
		"parse5": "^8.0.0",
		"svelte-chartjs": "^3.1.5",
		"undici": "^7.22.0"
	}
}
